{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "yml",
    "markdown"
  ],
  "cSpell.words": [
    "antfu",
    "axios",
    "bumpp",
    "chatgpt",
    "chenzhaoyu",
    "commitlint",
    "davinci",
    "dockerhub",
    "esno",
    "GPTAPI",
    "highlightjs",
    "hljs",
    "iconify",
    "katex",
    "katexmath",
    "linkify",
    "logprobs",
    "mdhljs",
    "mila",
    "nodata",
    "OPENAI",
    "pinia",
    "Popconfirm",
    "rushstack",
    "Sider",
    "tailwindcss",
    "traptitech",
    "tsup",
    "Typecheck",
    "unplugin",
    "VITE",
    "vueuse",
    "Zhao"
  ],
  "i18n-ally.enabledParsers": [
    "ts"
  ],
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.localesPaths": [
    "src/locales"
  ],
  "i18n-ally.keystyle": "nested",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#eedf43",
    "activityBar.background": "#eedf43",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#0d9286",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#eedf43",
    "statusBar.background": "#e9d615",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#baab11",
    "statusBarItem.remoteBackground": "#e9d615",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#e9d615",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#e9d61599",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#e9d615"
}
